import groovy.json.JsonSlurperClassic

def build_id

node () {
    stage('get stagger info') {
        checkout scm
        sh "pwd"
        sh "ls"
        sh "sh ./scripts/getamq.sh"
        def props = readProperties  file:'build.properties'
        build_id= props['BUILDID']
        echo "build_id=${build_id}"
    }
}